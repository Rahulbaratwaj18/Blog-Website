<%- include("partials/header"); %>

<h1>Compose a Post</h1>

<form  method="post" action="/compose">

    <div class="form-group">
        <label>Title</label>
        <input type="text" name="postTitle" placeholder="Add Title" autocomplete="off" class="form-control" >
    </div>
 
     
    <div class="form-group">
        <label>Post</label>
        <textarea id="post-content" name="postBody" row="10" cols="100" class="form-control"> </textarea>
    </div>
    

    <button type="submit" class="btn-publish" name="button">Publish</button>
</form>

<%- include("partials/footer"); %>
